{
    "editor.tabSize": 2,
    "editor.hover.delay": 1200,
    "eslint.options": {
        "extensions": [
            ".html",
            ".js",
            ".jsx",
            ".ts",
            ".tsx",
            ".json"
        ]
    },
    "eslint.format.enable": true,
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true
    },
    "eslint.alwaysShowStatus": true,
    "prettier.configPath": ".prettierrc"
}
